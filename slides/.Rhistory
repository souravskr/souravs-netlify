library(dplyr)
library(ggplot2)
insurance <- read.csv('insurance.csv', stringsAsFactors = FALSE)
ggplot(insurance, aes(x = bmi, y = charges, label = ifelse(smoker=='yes', 'Smoker','Non-Smoker'))) +
geom_point(alpha = 0.8, aes(color = sex)) +
geom_text_repel(
data          = subset(insurance, bmi > 50),
nudge_y       = 36 - subset(insurance, bmi > 50)$bmi,
segment.size  = 0.2,
segment.color = "grey40",
size = 4,
box.padding   = 0.35,
point.padding = 0.5
)
